{
  "name": "byjg/redis-queue-client",
  "description": "A minimal redis queue client using the components of byjg/message-queue-client",
  "autoload": {
    "psr-4": {
      "ByJG\\MessageQueueClient\\RedisQueue\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1 <8.5",
    "ext-curl": "*",
    "ext-redis": "*",
    "byjg/message-queue-client": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10|^11",
    "vimeo/psalm": "^5.9|^6.12"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm"
  },
  "license": "MIT"
}
